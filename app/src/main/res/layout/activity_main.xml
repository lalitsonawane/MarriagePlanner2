<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.lalitsonawane.admin.marriageplanner.MainActivity">

    <TextView
        android:text="bride"
        android:layout_width="106dp"
        android:layout_height="18dp"
        android:id="@+id/bride"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="@+id/editText5"
        app:layout_constraintBottom_toBottomOf="@+id/editText5" />

    <TextView
        android:text="groom"
        android:layout_width="106dp"
        android:layout_height="16dp"
        android:id="@+id/groom"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/bride"
        android:layout_marginTop="56dp" />

    <EditText
        android:layout_width="217dp"
        android:layout_height="44dp"
        android:inputType="textPersonName"
        android:text="Name"
        android:ems="10"
        android:id="@+id/editText5"
        app:layout_constraintLeft_toRightOf="@+id/groom"
        android:layout_marginStart="48dp"
        app:layout_constraintTop_toTopOf="@+id/activity_main"
        android:layout_marginTop="16dp" />

    <EditText
        android:layout_width="217dp"
        android:layout_height="44dp"
        android:inputType="textPersonName"
        android:text="Name"
        android:ems="10"
        android:id="@+id/editText6"
        app:layout_constraintTop_toBottomOf="@+id/editText5"
        android:layout_marginTop="43dp"
        app:layout_constraintRight_toRightOf="@+id/editText5" />

    <Button
        android:text="register"
        android:layout_width="117dp"
        android:layout_height="wrap_content"
        android:id="@+id/register"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@+id/editText6"
        android:layout_marginTop="44dp"
        app:layout_constraintRight_toRightOf="@+id/activity_main"
        android:layout_marginEnd="16dp" />

    <TextView
        android:text="TextView"
        android:layout_width="216dp"
        android:layout_height="29dp"
        tools:layout_editor_absoluteY="346dp"
        android:id="@+id/textView"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        android:layout_marginStart="16dp" />

    <TextView
        android:text="TextView"
        android:layout_width="138dp"
        android:layout_height="29dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-81dp"
        android:id="@+id/textView2" />

    <TextView
        android:text="TextView"
        android:layout_width="215dp"
        android:layout_height="30dp"
        android:id="@+id/textView3"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="392dp" />

</android.support.constraint.ConstraintLayout>
